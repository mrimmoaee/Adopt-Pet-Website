hParams.js","original":{"line":6,"column":0}},{"name":"SearchParams","generated":{"line":79,"column":15},"source":"SearchParams.js","original":{"line":43,"column":15}}],"sources":{"SearchParams.js":"import React, { useState, useEffect } from \"react\";\nimport pet, { ANIMALS } from \"@frontendmasters/pet\";\nimport useDropdown from \"./useDropdown\";\nimport Pet from \"./Pet\";\n\nconst SearchParams = () => {\n  const [location, setLocation] = useState(\"Seattle, WA\");\n  const [breeds, setBreeds] = useState([]);\n  const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n  const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\n\n  useEffect(() => {\n    setBreeds([]);\n    setBreed(\"\");\n\n    pet.breeds(animal).then(({ breeds }) => {\n      const breedStrings = breeds.map(({ name }) => name);\n      setBreeds(breedStrings);\n    }, console.error);\n  }, [animal, setBreed, setBreeds]);\n\n  return (\n    <div className=\"search-params\">\n      <form>\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={e => setLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n\n        <button>Submit</button>\n      </form>\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"f1753a57b58c12a4281b9a6239a1d1d9","cacheData":{"env":{}}}